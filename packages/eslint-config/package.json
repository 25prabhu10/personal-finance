{
  "name": "@pf/eslint-config",
  "version": "0.0.1",
  "private": true,
  "description": "ESLint configuration for Personal Finance management tool",
  "keywords": [
    "eslint"
  ],
  "author": "Prabhu",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "type": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@pf/typescript-config": "workspace:*",
    "eslint": "^9.24.0",
    "eslint-plugin-package-json": "^0.29.1",
    "eslint-plugin-perfectionist": "^4.11.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "packageManager": "pnpm@10.8.1",
  "engines": {
    "node": ">=23"
  },
  "title": "Personal Finance"
